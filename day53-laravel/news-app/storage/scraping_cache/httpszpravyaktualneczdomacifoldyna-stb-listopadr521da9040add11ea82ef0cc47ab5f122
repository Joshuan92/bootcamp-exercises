		(function(w) {
			var fatalFail = true;
			var hasBbxLibrary = null;
			var ft = [];
			try {
				var c = BBX.cookies.get('bbxuid');
				ft.push('bc=' + (c || 'warn_no_cookie'));
				hasBbxLibrary = true;
			} catch(e) {
				ft.push('bc=fail_bbx_cook');
			}
			try {
				var queuePush = SiteAdvert.Queue.push;
				if (typeof queuePush === 'function') {
					if (queuePush.toString().match(/_sashec_queue\.push/)) {
						ft.push('bq=fail_original');
					} else if (queuePush.toString().match(/\.size/)) {
						ft.push('bq=seems_ok');
						fatalFail = false;
					} else {
						ft.push('bq=fail_size_parse_missing');
					}
				} else {
					ft.push('bq=fail_queue_not_func');
				}
			} catch(e) {
				ft.push('bq=fail_queue');
			}
			try {
				ft.push('sat=' + _satellite.getVar('bbxDevice'));
			} catch(e) {
				ft.push('sat=fail_satellite');
			}
			var device = 'x';
			try {
				var s = _sashec.ad_groups.default.options;
				var lt = [
					'sd=' + s.targets.device,
					'st=' + s.targets.template,
					'ss=' + s.site,
					'sa=' + s.area,
					'sb=' + s.targets.ab
				];
				device = s.targets.device;
				ft.push(lt.join('&'));
				if (w.bbxtrck.active.sasSpeed) {
					var origRenderPositions = _sashec.Postscribe.renderPositions;
					var spcount = 0;
					var sizes = {
						'mediumrectangle': 'mr',
						'prannotation': 'pr',
						'square': 'sq',
						'wallpaper': 'wp',
						'halfpage': 'hp',
						'special': 'sp',
						'commercial': 'cm',
						'leader': 'ldr',
						'prpromo': 'pp',
						'fullbanner': 'fb',
						'megaboard': 'meg',
						'strip': 'st',
						'widesky': 'ws'
					};

					var compressSizes = function(src) {
						var s = src.match(/size=[^\/]+/g) || [];
						try {
							s = s.map(function(item) { return item.replace('size=', ''); });
							for (var repl in sizes) {
								for (var i = 0, len = s.length; i<len; i++) {
									s[i] = s[i].replace(repl, sizes[repl]);
								}
							}
						} catch(e) {
						}
						return s.join('+');
					};

					_sashec.Postscribe.renderPositions = function() {
						var args = arguments;
						var data = [];
						var src = '__nesmysl__';
						try {
							src = args[0].src;
						} catch(e) {
							src = '__nesmysl__';
						}
						spcount++;
						data.push('spc=' + spcount);
						data.push('siz=' + compressSizes(src));
						try {
							var items = w.performance.getEntries().filter(function(item) {
								return item.name == src;
							});
							var device = src.match(/device=([^\/]+)\//);
							device = device && device[1] || 'x';
							data.push('sd=' + device);
							data.push('rsn=normal');
							data.push('spst=' + parseInt(items[0].fetchStart, 10));
							data.push('spen=' + parseInt(items[0].responseEnd, 10));
							w.bbxtrck.track('sasSpeed', '&' + data.join('&'), true);
						} catch(e) {
							data.push('rsn=fail_perf');
							data.push('spen=' + parseInt(w.bbxtrck.getPerfTime(), 10));
							w.bbxtrck.track('sasSpeed', '&' + data.join('&'), true);
						}
						// call original code
						return origRenderPositions.apply(this, args);
					}
				}
			} catch(e) {
				ft.push('sd=fail_sashec');
			}
			try {
				ft.push('ie=' + (w.document.documentMode || 'no'));
			} catch (e) {
				ft.push('ie=cannot_detect');
			}
			var noads = false;
			var noop = function() { };
			w.bbxtrck.expectedLeader = noop;
			w.bbxtrck.test = noop;
			w.bbxtrck.fatalFail = fatalFail;
			w.bbxtrck.hasBbxLibrary = hasBbxLibrary;
			w.bbxtrck.active.resource && w.bbxtrck.track('resource', '&' + ft.join('&'));
			if(w.bbxtrck.active.unload && w.bbxtrck.hasBeacon) {
				var once = true;
				var unload = function() {
					if (!once) {
						return;
					}
					once = false;
					if(w.bbxtrck.test !== noop && !w.bbxtrck.test()) {
						var bad = w.bbxtrck.getTrackUri('badLeader', '&rsn=unload&sd=' + device);
						bad && navigator.sendBeacon(bad);
					}
					var info = w.bbxtrck.getTrackUri('unload', '&sd=' + device);
					info && navigator.sendBeacon(info);
				};
				w.addEventListener('beforeunload', unload);
				w.addEventListener('unload', unload);
			}
			if (hasBbxLibrary) {
				BBX.ready(function() {
					w.bbxtrck.active.dom && w.bbxtrck.track('dom');
					if (!w.bbxtrck.active.leader) {
						return;
					}
					if (!document.getElementById('sas-leader')) {
						w.bbxtrck.track('noLeader', '&rsn=no_in_page&sd=' + device);
					} else if(noads) {
						w.bbxtrck.track('noLeader', '&rsn=no_ads&sd=' + device);
					} else {
						try {
							var leader = _sashec.getPositionsByElementId('sas-leader');
							if (leader.length === 0) {
								if (fatalFail) {
									w.bbxtrck.track('noLeader', '&rsn=fail_queue_register&sd=' + device);
								} else {
									w.bbxtrck.track('noLeader', '&rsn=position_not_registered&sd=' + device);
								}
								return;
							}
							var size = leader[0].options.size;
							if (typeof size === 'function') {
								w.bbxtrck.track('badLeader', '&rsn=size_is_function&sd=' + device);
							} else if (size == null) {
								w.bbxtrck.track('noLeader', '&rsn=size_is_null&sd=' + device);
							} else if (size.toString().indexOf('leader') == -1) {
								w.bbxtrck.track('noLeader', '&rsn=size_without_leader&sd=' + device);
							} else {
								w.bbxtrck.track('expectedLeader', '&sd=' + device);
								w.bbxtrck.test = function() {
									var isSSP = _sashec.Postscribe.activeQueue[Object.keys(_sashec.Postscribe.activeQueue)]['data'].filter(function(item) {
										return item.elementId === 'sas-leader' && /data-rp-size-id=([^"'\d]*['"]?)230\1[^\d]/i.test(item.code);
									}).length > 0;
									if (isSSP) {
										w.bbxtrck.track('successLeader', '&rsn=ssp&sd=' + device);
										return true;
									}
									var beacon = w.performance.getEntries().filter(function(item) {
										return item.name.indexOf('https://a.centrum.cz') === 0
										  && item.name.indexOf('cent/count/FCID') > 0
										  && item.name.indexOf('/ACT=4') == -1 
										  && item.name.indexOf('/size=leader') > 0;
									});
									if (beacon.length > 0) {
										var fcid = beacon[0].name.match(/FCID=(\d+)\//i);
										var beaStart = parseInt(beacon[0].fetchStart, 10);
										var beaEnd = parseInt(beacon[0].responseEnd, 10);
										fcid = fcid && fcid[1] || 0;
										w.bbxtrck.track('successLeader', '&rsn=normal&beas=' + beaStart + '&beae=' + beaEnd + '&fcid=' + fcid + '&sd=' + device);
										return true;
									}
									return false;
								}
								w.bbxtrck.expectedLeader = function() {
									var interval, timeout;
									interval = setInterval(function() {
										try {
											if (w.bbxtrck.test()) {
												clearInterval(interval);
												clearTimeout(timeout);
											}
										} catch(e) {
											clearInterval(interval);
											clearTimeout(timeout);
											w.bbxtrck.track('badLeader', '&rsn=cannot_detect&sd=' + device);
										}
											

									}, 250);
									timeout = setTimeout(function() {
										clearInterval(interval);
										w.bbxtrck.track('badLeader', '&rsn=timeout_50s&sd=' + device);
									}, 50000);
								};
							}
						} catch(e) {
							w.bbxtrck.track('badLeader', '&rsn=sashec_error&sd=' + device);
						}
					}
				});
			} else {
				w.bbxtrck.active.bbxError && w.bbxtrck.track('bbxError', '&rsn=fail_bbx_lib');
			}
			w.bbxtrck.active.load && w.addEventListener('load', function() {
				w.bbxtrck.track('load', '&sd=' + device);
			});
		})(window);
		</script>
</head>

