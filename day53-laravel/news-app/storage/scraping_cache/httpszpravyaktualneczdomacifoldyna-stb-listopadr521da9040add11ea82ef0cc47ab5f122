				SiteAdvert.Queue.push(['position', 'sas-multisize-3', {
					size: function (device) {
						if (device === 'm' && 3 !== 3) {
							return { size: ['mediumrectangle', 'square300'] }
						} else if (device === 'm' && 3 === 3) {
							return { size: null };
						} else {
							return { size: ['halfpage', 'mediumrectangle', 'square300', 'widesky'] }
						}
					},
